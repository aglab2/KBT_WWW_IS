        <!--
            CJK Symbols and Punctuation 3000-303F
            Hiragana                    3040-309F
            Katakana                    30A0-30FF
            Katakana Phonetic Ext.      31F0-31FF -->

        <!-- CHS -->
        <!-- On Vista, the CHS UI font has a sans-serif style; it supports these except for 31F0-31FF. -->
        <!-- On Vista, MS Gothic supports 31F0-31FF; SimSun does not. -->
        <!-- On XP, the CHS UI font has a serif style; it has gaps in 3000-303F, 3040-309F and 30A0-30FF,
             some of which are covered by MS Gothic. MS Gothic also has gaps; some in 3000-303F are covered
             by SimSun. -->
        <!-- More important to match the style of punctuation used for CHS with ideographs than to keep
             serif/sans-serif consistency across punctuation/symbols or across kana. -->
        <FontFamilyMap  
            Unicode          = "3000-30FF, 31F0-31FF"
            Language         = "zh-Hans"
            Target           = "Microsoft YaHei UI, Microsoft YaHei, SimSun, MS Gothic, Arial Unicode MS"
            Scale            = "1.0"/>

        <!-- CHT -->
        <!-- On Vista, the CHT UI font has a sans-serif style; it supports these except for 31F0-31FF. -->
        <!-- On Vista, MS Gothic supports 31F0-31FF; MingLiU does not. -->
        <!-- On XP, the CHT UI font has a serif style; it has gaps in 3000-303F, 3040-309F and 30A0-30FF,
             some of which are covered by MS Gothic. MS Gothic also has gaps; some in 3000-303F are covered
             by MingLiU. -->
        <!-- More important to match the style of punctuation used for CHS with ideographs than to keep
             serif/sans-serif consistency across punctuation/symbols or across kana. -->
        <FontFamilyMap  
            Unicode          = "3000-30FF, 31F0-31FF"
            Language         = "zh-Hant"
            Target           = "Microsoft JhengHei UI, Microsoft JhengHei, MingLiu, MS Gothic, Arial Unicode MS"
            Scale            = "1.0"/>

